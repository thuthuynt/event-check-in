{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "event-check-in",
  "main": "./workers/app.ts",
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "vars": {
    "VALUE_FROM_CLOUDFLARE": "Hello from Cloudflare",
    "JWT_SECRET": "your-super-secret-jwt-key-change-this-in-production"
  },
  "d1_databases": [
    {
      "binding": "DUCKLYTICS_PROD",
      "database_name": "ducklytics-db",
      "database_id": "30bf22a3-601f-462a-95e0-40dfd0d49016"
    }
  ],
  "r2_buckets": [
    {
      "binding": "EVENT_STORAGE",
      "bucket_name": "event-check-in-storage"
    }
  ],
  "routes": [
    {
      "pattern": "check-in.ducklytics.com/*",
      "zone_name": "ducklytics.com"
    }
  ]
}
